if __name__ == '__main__':
    x = int(raw_input())
    y = int(raw_input())
    z = int(raw_input())
    n = int(raw_input())
    print([[a,b,c] for a in range(x+1) for b in range(y+1) for c in range(z+1) if a+b+c != n])

method 2

    x = int(input())
    y = int(input())
    z = int(input())
    n = int(input())
    l=[]
    for i in range(x+1):
        for j in range(y+1):
            for k in range(z+1):
                if i+j+k !=n:
                    l.append([i,j,k])
    print(l)
